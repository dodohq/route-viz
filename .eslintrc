{
  "parser": "babel-eslint",
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "error",
    "no-console": "error",
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error"
  },
  "extends": ["prettier"],
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "fetch": true
  }
}
